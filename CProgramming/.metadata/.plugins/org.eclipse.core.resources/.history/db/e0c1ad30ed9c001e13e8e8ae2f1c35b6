#include <stdio.h>
#include <stdlib.h>

int main(){
int x = 5;
int y[3] = {1,2,3};
char z[2] = {'h' , 0};
printf("%d \n" , x);
printf("%d \n " , &x);
printf("%d \n" , y);
printf("%d \n" , &y);
printf("%d \n" , z);
printf("%d" , &z);
return 0;
}
