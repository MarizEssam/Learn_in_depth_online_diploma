#include <stdio.h>
#include <stdlib.h>

int main(){
	int n,i,x[50],number , loc;
	printf("Enter number of array: ");
	fflush(stdout);
	printf("enter data: ");
	fflush(stdout);
	for(i=0;i<n;i++){
		scanf("%d",&x[i]);
		printf("%d",x[i]);
		if(i==n-1) printf("\n");
	}
	printf("enter element to be inserted: \n");
	scanf("%d",number);
	printf("enter location: \n");
	scanf("%d",&loc);
}
