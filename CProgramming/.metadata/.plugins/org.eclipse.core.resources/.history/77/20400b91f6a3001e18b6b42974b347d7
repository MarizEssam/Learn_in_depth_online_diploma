#include <stdio.h>
#include <stdlib.h>

int main(){
	int n,i,x[50],s;
	printf("Enter number of array: ");
	fflush(stdout);
	scanf("%d",&n);

	printf("enter data: ");
	fflush(stdout);
	for(i=0;i<n;i++){
		scanf("%d",&x[i]);
}
	printf("enter element to be searched: ");
	fflush(stdout);
	scanf("%d",&s);
	printf("%d",s);
}
